{
    "name": "popphp/pop-acl",
    "description": "Pop ACL Component for Pop PHP Framework",
    "keywords": [
        "acl",
        "access control list",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "https://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "https://www.popphp.org/"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "popphp/pop-utils": "^2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Acl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Acl\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1.x-dev"
        }
    }
}
